{
  "name": "your-script",
  "version": "0.1.34",
  "description": "A Tampermonkey script with automated version control",
  "type": "module",
  "scripts": {
    "build": "node build.js",
    "semantic-release": "semantic-release",
    "prebuild": "node increment-version.js",
    "upload-to-jianguoyun": "node uploadToJianguo.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taigongzhaihua/TampermonkeyTJFZXT.git"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.1",
    "@semantic-release/npm": "^7.1.3",
    "semantic-release": "^17.4.7",
    "webdav": "^5.6.0"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "README.md",
            "CHANGELOG.md",
            "dist/**/*"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "dependencies": {
    "axios": "^1.6.8",
    "moment": "^2.30.1"
  }
}